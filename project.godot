; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_invalid_track_paths=false

[application]

config/name="Im Coming, Hub"
run/main_scene="res://Features/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GlobalEnums="*res://Features/global_enums.gd"
Database="*res://Features/Database.gd"
SfxManager="*res://Scripts/SFXManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/script-ide/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=3
textures/canvas_textures/default_texture_repeat=1
textures/vram_compression/import_etc2_astc=true
viewport/hdr_2d=true
anti_aliasing/quality/msaa_2d=2
anti_aliasing/quality/screen_space_aa=1
